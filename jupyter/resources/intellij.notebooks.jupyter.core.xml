<idea-plugin package="com.intellij.notebooks.jupyter.core">
  <!-- Jupyter language -->
    <extensions defaultExtensionNs="com.intellij">
      <fileType name="Jupyter"
                language="Jupyter"
                extensions="ipynb"
                implementationClass="com.intellij.notebooks.jupyter.core.jupyter.JupyterFileType"
                fieldName="INSTANCE"/>
    </extensions>
</idea-plugin>
