<idea-plugin package="com.intellij.searchEverywhereMl.semantics.python">
  <dependencies>
    <plugin id="com.intellij.modules.python"/>
    <module name="intellij.searchEverywhereMl.semantics"/>
  </dependencies>

  <extensions defaultExtensionNs="com.intellij.embeddings.indexer">
    <classesProvider filetype="Python" implementationClass="com.intellij.searchEverywhereMl.semantics.python.PythonClassesProvider"/>
    <symbolsProvider filetype="Python" implementationClass="com.intellij.searchEverywhereMl.semantics.python.PythonSymbolsProvider"/>
  </extensions>
</idea-plugin>