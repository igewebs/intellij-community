<idea-plugin package="com.intellij.searchEverywhereMl.semantics.java">
  <dependencies>
    <plugin id="com.intellij.java"/>
    <module name="intellij.searchEverywhereMl.semantics"/>
  </dependencies>

  <extensions defaultExtensionNs="com.intellij.embeddings.indexer">
    <classesProvider filetype="JAVA" implementationClass="com.intellij.searchEverywhereMl.semantics.java.JavaClassesProvider"/>
    <symbolsProvider filetype="JAVA" implementationClass="com.intellij.searchEverywhereMl.semantics.java.JavaSymbolsProvider"/>
  </extensions>
</idea-plugin>